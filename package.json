{
  "name": "feed",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "check-types": "turbo run check-types",
    "init-db": "pnpm --filter @feed/api init-db",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint:fix": "turbo run lint -- --fix",
    "lint": "turbo run lint",
    "start": "turbo run start"
  },
  "dependencies": {
    "typebox": "1.0.51"
  },
  "devDependencies": {
    "prettier": "3.6.2",
    "eslint": "9.39.1",
    "turbo": "2.6.0",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}